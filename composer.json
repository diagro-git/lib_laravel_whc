{
    "name": "diagro/lib_laravel_whc",
    "description": "Library for webhook clients for backend applications",
    "type": "library",
    "version": "10.0.0",
    "license": "Beerware",
    "authors": [
        {
            "name": "Stijn Leenknegt",
            "email": "stijn@diagro.be"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework":"^10.0",
        "diagro/lib_laravel_token": "^10.0.0",
        "spatie/laravel-webhook-client": "3.4.3",
        "guzzlehttp/guzzle": "^7.9.3"
    },
    "autoload": {
        "psr-4": {
            "Diagro\\Webhooks\\Client\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Diagro\\Webhooks\\Client\\DiagroServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
